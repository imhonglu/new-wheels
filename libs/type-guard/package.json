{
	"name": "@imhonglu/type-guard",
	"version": "1.0.0",
	"description": "TypeScript type guard utilities providing runtime type checking and validation with zero dependencies",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imhonglu/new-wheels.git",
		"directory": "libs/type-guard"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"test": "vitest",
		"build": "tsc -p tsconfig.build.json",
		"postbuild": "esm-to-cjs && pnpm api-extractor run && pnpm api-documenter markdown -i ./temp -o ./docs"
	},
	"keywords": [
		"typescript",
		"utilities",
		"type-safe",
		"type-guard",
		"zero-dependencies"
	],
	"author": "imhonglu <anonymouser.unknown@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@imhonglu/build-tools": "^1.0.0",
		"@imhonglu/configs": "^1.0.2"
	}
}
